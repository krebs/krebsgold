{
	"name": "__MSG_appName__",
	"short_name": "__MSG_shortName__",
	"version": "1.3.4",
	"manifest_version": 2,
	"description": "__MSG_appDescription__",
	"icons": {
		"16": "images/icon-16.png",
		"128": "images/icon-128.png"
	},
	"default_locale": "en",
	"background": {
		"scripts": [
			"scripts/background.js"
		]
	},
	"permissions": [
		"tabs",
		"webNavigation",
		"storage",
		"webRequest",
		"webRequestBlocking",
		"*://www.amazon.de/*",
		"*://www.amazon.co.uk/*",
		"*://www.amazon.com/*",
		"*://www.amazon.fr/*",
		"*://www.amazon.co.jp/*",
		"*://www.amazon.ca/*",
		"*://www.amazon.cn/*",
		"*://www.amazon.it/*",
		"*://www.amazon.es/*",
		"*://www.amazon.in/*",
		"*://www.amazon.com.br/*"
	],
	"browser_action": {
		"default_icon": {
			"38": "images/icon-32.png"
		},
		"default_title": "Krebsgold",
		"default_popup": "popup.html"
	}
}
